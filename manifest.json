{
  "manifest_version": 3,
  "name": "SafeSurf",
  "version": "1.0.0",
  "description": "An extension to keep children safe from innapropriate content",
  "content_scripts": [
    {
      "js": ["js/popup.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": "icons/itachi.png",
    "default_popup": "html/safeSurf.html"
  },
  "permissions": ["webRequest", "tabs"],
  "host_permissions": ["<all_urls>"]
}
