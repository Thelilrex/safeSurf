{
  "manifest_version": 3,
  "name": "SafeSurf",
  "version": "1.0.0",
  "description": "An extension to keep children safe from innapropriate content",
  "content_scripts": [
    {
      "default_icon": "icons/itachi.png",
      "js": ["js/popup.js"],
      "matches": ["<all_urls>"],
      "html": ["html/safeSurf.html"]
    }
  ],
  "permissions": ["webRequest", "tabs"],
  "host_permissions": ["<all_urls>"]
}
